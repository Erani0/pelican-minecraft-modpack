{
    "name": "timvida/pelican-minecraft-modpack",
    "description": "Browse and install Minecraft modpacks from multiple platforms in Pelican Panel",
    "type": "pelican-plugin",
    "keywords": [
        "pelican",
        "pelican-panel",
        "minecraft",
        "modpacks",
        "plugin",
        "modrinth",
        "curseforge",
        "atlauncher",
        "feedthebeast",
        "technic"
    ],
    "homepage": "https://github.com/timvida/pelican-minecraft-modpack",
    "license": "MIT",
    "authors": [
        {
            "name": "timvida",
            "homepage": "https://github.com/timvida"
        }
    ],
    "support": {
        "issues": "https://github.com/timvida/pelican-minecraft-modpack/issues",
        "source": "https://github.com/timvida/pelican-minecraft-modpack"
    },
    "require": {
        "php": "^8.1|^8.2|^8.3"
    },
    "autoload": {
        "psr-4": {
            "TimVida\\MinecraftModpacks\\": "src/"
        }
    },
    "extra": {
        "pelican": {
            "plugin": "TimVida\\MinecraftModpacks\\MinecraftModpacksPlugin"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
